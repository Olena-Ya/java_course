package loops;

public class loops {
	
	
	public void task1 () {

		for (int i=1; i<=11; i++)
		{
			System.out.print (i +" ");
					
			
		}	
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		loops loopsMethod  = new loops();
		
		for (int i=1; i<=4; i++)
		{
			System.out.println ("Outer loop started");
			
			
			for (int j=1; j<=4; j++)
			{
				System.out.println ("Inner loop");
				
			}
			
			System.out.println ("Outer loop finished");
		}
		
		loopsMethod.task1();
}
}
